se},"description":"page","allowEmptyValue":false,"additionalProperties":{"$ref":"#/definitions/FilterConstraint"}},"fields":{"type":"array","xml":{"name":"fields","attribute":false,"wrapped":false},"description":"fields","allowEmptyValue":false,"items":{"type":"string"}},"franchiseeNo":{"type":"array","items":{"type":"integer","format":"int32"}},"invoiceDate":{"type":"array","items":{"type":"string","format":"date-time"}},"page":{"xml":{"name":"page","attribute":false,"wrapped":false},"description":"page","allowEmptyValue":false,"$ref":"#/definitions/Page"},"paymentStatus":{"$ref":"#/definitions/Character"}},"title":"RoyaltyFilter","description":"Provides the filter parameter"},"SalesFilter":{"type":"object","properties":{"constraints":{"type":"object","example":"format: {db_field:{operation:dataValue}","xml":{"name":"constraints","attribute":false,"wrapped":false},"description":"page","allowEmptyValue":false,"additionalProperties":{"$ref":"#/definitions/FilterConstraint"}},"fields":{"type":"array","xml":{"name":"fields","attribute":false,"wrapped":false},"description":"fields","allowEmptyValue":false,"items":{"type":"string"}},"franchiseeNo":{"type":"array","items":{"type":"integer","format":"int32"}},"page":{"xml":{"name":"page","attribute":false,"wrapped":false},"description":"page","allowEmptyValue":false,"$ref":"#/definitions/Page"},"reportPeriodEnd":{"type":"array","items":{"type":"string","format":"date-time"}},"reportPeriodStart":{"type":"array","items":{"type":"string","format":"date-time"}},"reportSubmitDate":{"type":"array","items":{"type":"string","format":"date-time"}},"reportType":{"type":"array","items":{"$ref":"#/definitions/Character"}},"salesReportID":{"type":"array","items":{"type":"integer","format":"int32"}},"totalSales":{"type":"array","items":{"type":"integer","format":"int32"}}},"title":"SalesFilter","description":"Provides the filter parameter"},"Sort":{"type":"object","properties":{"columns":{"type":"array","items":{"type":"string"}},"direc